<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="mayeco_notifications_response" path="/process_getresponse">
        <default key="_controller">MayecoNotificationsBundle:Notifications:getResponse</default>
        <requirement key="_method">GET</requirement>
    </route>

    <route id="mayeco_notifications_notification" path="/process_notification">
        <default key="_controller">MayecoNotificationsBundle:Notifications:getNotification</default>
        <requirement key="_method">POST</requirement>
    </route>

</routes>
